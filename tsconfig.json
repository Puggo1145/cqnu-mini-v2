{
  "extends": "@vue/tsconfig/tsconfig.json",
  "compilerOptions": {
    "verbatimModuleSyntax": true,
    "importsNotUsedAsValues": "remove",
    "preserveValueImports": null,
    "sourceMap": true,
    "baseUrl": ".",
    "paths": {
      "@/*": ["./src/*"],
    },
    "lib": ["esnext", "dom"],
    "types": [
      "@dcloudio/types",
      "uview-plus/types"
    ]
  },
  "include": ["src/**/*.ts", "src/**/*.d.ts", "src/**/*.tsx", "src/**/*.vue"]
}
